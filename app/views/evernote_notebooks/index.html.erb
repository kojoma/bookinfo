<% flash.each do |name, msg| %>
  <div class="alert alert-<%= name == "notice" ? "success" : "error" %>">
    <a class="close" data-dismiss="alert">×</a> <!-- アラートダイアログに×ボタンをつける -->
    <%= msg %>
  </div>
<% end %>

<%- model_class = EvernoteNotebook -%>
<div class="page-header">
  <h1>Evernote送信先登録</h1>
</div>

<%= render :partial => 'form' %>
